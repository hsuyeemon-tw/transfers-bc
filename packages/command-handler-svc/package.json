{
  "name": "@mojaloop/transfers-bc-command-handler-svc",
  "version": "0.2.9",
  "description": "Mojaloop vNext Transfers BC - Command Handler Service",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/transfers-bc/command-handler-svc",
  "private": true,
  "mojaloop": {
    "publish_to_dockerhub": true
  },
  "bugs": {
    "url": "https://github.com/mojaloop/transfers-bc/issues#readme"
  },
  "contributors": [
    "Pedro Sousa Barreto <pedrosousabarreto@gmail.com>"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "npm run service",
    "start:dev": "npm run build && AUDIT_KEY_FILE_PATH=./dist/tmp_audit_key_file npm run service",
    "service": "node dist/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "(npm run clean:npm || true) && npm run clean:dist",
    "clean:dist": "rm -rf dist",
    "clean:npm": "rm -rf node_modules",
    "pretest": "npm run build",
    "standard": "npm run lint",
    "lint": "npm run build && npx eslint ./src",
    "lint:fix": "npm run build && npx eslint ./src --fix",
    "test:unit": "jest --passWithNoTests --testMatch **/test/unit/**/*.test.ts",
    "dep:check": "ncu -e 2",
    "dep:update": "ncu -u"
  },
  "dependencies": {
    "@mojaloop/auditing-bc-client-lib": "~0.0.15",
    "@mojaloop/logging-bc-client-lib": "^0.1.26",
    "@mojaloop/platform-configuration-bc-client-lib": "~0.3.4",
    "@mojaloop/platform-configuration-bc-public-types-lib": "^0.3.3",
    "@mojaloop/platform-shared-lib-nodejs-kafka-client-lib": "^0.3.67",
    "@mojaloop/platform-shared-lib-observability-client-lib": "~0.1.10",
    "@mojaloop/platform-shared-lib-observability-types-lib": "~0.1.9",
    "@mojaloop/platform-shared-lib-public-messages-lib": "~0.1.154",
    "@mojaloop/security-bc-client-lib": "~0.2.3",
    "@mojaloop/transfers-bc-domain-lib": "*",
    "@mojaloop/transfers-bc-implementations-lib": "*",
    "express": "^4.18.2",
    "@mojaloop/transfers-bc-config-lib": "~0.1.0"
  },
  "devDependencies": {
    "@mojaloop/logging-bc-public-types-lib": "^0.1.20",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "npm-check-updates": "^16.10.19",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
