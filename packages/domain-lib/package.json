{
  "name": "@mojaloop/transfers-bc-domain-lib",
  "version": "0.0.1",
  "description": "Mojaloop vNext Transfers BC - Domain lib",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/transfers-bc/domain-lib",
  "private": true,
  "bugs": {
    "url": "https://github.com/mojaloop/transfers-bc/issues#readme"
  },
  "contributors": [
    "Pedro Sousa Barreto <pedrosousabarreto@gmail.com>"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean:npm": "rm -Rf node_modules",
    "clean": "rm -Rf dist",
    "pretest": "npm run build",
    "standard": "npm run lint",
    "lint": "npm run build && npx eslint ./src",
    "lint:fix": "npm run build && npx eslint ./src --fix",
    "test:unit": "jest --passWithNoTests --testMatch '**/test/unit/**/*.test.ts'",
    "dep:check": "ncu -e 2",
    "dep:update": "ncu -u"
  },
  "dependencies": {
    "@mojaloop/accounts-and-balances-bc-public-types-lib": "~0.1.2",
    "@mojaloop/auditing-bc-public-types-lib": "~0.0.11",
    "@mojaloop/logging-bc-public-types-lib": "~0.1.14",
    "@mojaloop/platform-shared-lib-public-messages-lib": "~0.1.68",
    "@mojaloop/security-bc-public-types-lib": "~0.1.3",
    "@mojaloop/participant-bc-public-types-lib":"~0.0.7"
  },
  "devDependencies": {
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "npm-check-updates": "^16.3.11",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
