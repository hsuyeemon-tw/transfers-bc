{
  "name": "@mojaloop/transfers-bc-domain-lib",
  "version": "0.2.0",
  "description": "Mojaloop vNext Transfers BC - Domain lib",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/transfers-bc/domain-lib",
  "private": true,
  "bugs": {
    "url": "https://github.com/mojaloop/transfers-bc/issues#readme"
  },
  "contributors": [
    "Pedro Sousa Barreto <pedrosousabarreto@gmail.com>"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "npm run clean:npm; npm run clean:dist",
    "clean:dist": "rm -rf dist",
    "clean:npm": "rm -rf node_modules",
    "pretest": "npm run build",
    "standard": "npm run lint",
    "lint": "npm run build && npx eslint ./src",
    "lint:fix": "npm run build && npx eslint ./src --fix",
    "test:unit": "jest --passWithNoTests",
    "dep:check": "ncu -e 2",
    "dep:update": "ncu -u"
  },
  "dependencies": {
    "@mojaloop/accounts-and-balances-bc-public-types-lib": "~0.2.2",
    "@mojaloop/auditing-bc-public-types-lib": "~0.0.17",
    "@mojaloop/logging-bc-public-types-lib": "~0.1.20",
    "@mojaloop/participant-bc-public-types-lib": "~0.0.16",
    "@mojaloop/platform-shared-lib-messaging-types-lib": "~0.2.101",
    "@mojaloop/platform-shared-lib-observability-types-lib": "~0.1.9",
    "@mojaloop/platform-shared-lib-public-messages-lib": "~0.1.154",
    "@mojaloop/security-bc-public-types-lib": "~0.2.1",
    "@mojaloop/scheduling-bc-public-types-lib": "~0.0.5",
    "@mojaloop/transfers-bc-config-lib": "~0.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "npm-check-updates": "^16.10.19",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
